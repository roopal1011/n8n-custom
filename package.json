{
  "name": "n8n-nodes-roopal-custom",
  "version": "0.1.0",
  "description": "Custom n8n nodes for integrations and API workflows",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "custom-node",
    "workflow"
  ],
  "license": "MIT",
  "homepage": "https://github.com/roopal1011/n8n-custom",
  "author": {
    "name": "Roopal Rastogi",
    "email": "your-email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roopal1011/n8n-custom.git"
  },
  "main": "index.js",
  "scripts": {
    "install-n8n": "npm install && npm install -g n8n@1.73.1",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "test": "npm run build && n8n start"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/nodes/BinancePrice/BinancePrice.node.js",
      "dist/credentials/ExampleCredentialsApi.credentials.js",
      "dist/credentials/HttpBinApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ExampleNode/ExampleNode.node.js",
      "dist/nodes/HttpBin/HttpBin.node.js",
      "dist/nodes/HttpVerb/HttpVerb.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.70.0",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}